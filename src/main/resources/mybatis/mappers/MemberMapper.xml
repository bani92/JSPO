<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jspo.member.dao.MemberDao">

<!--    회원가입-->
    <insert id="insertMember" parameterType="MemberDto">
        insert into member (m_email, m_pwd, m_name, m_birth, m_phone)
        values (#{m_email}, #{m_pwd}, #{m_name}, #{m_birth}, #{m_phone});
    </insert>

<!--    회원조회-->
    <select id="selectMember" parameterType="MemberDto" resultType="MemberDto">
        select * from member where m_email = #{m_email};
    </select>


</mapper>